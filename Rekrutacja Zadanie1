print('Wprowadź liczbę:')
a = int(input())
if a%4==0:
  print("Ta liczba jest wielokrotnością 4")
elif a%2==0:
    print("Ta liczba jest parzysta")
else:
    print('Ta liczba jest nieparzysta')

print('Wprowadź num1:')
num1 = int(input())
print('Wprowadź num2:')  
num2 = int(input())

if num1%num2==0:
  print('Num1 dzieli się przez num2')
else:
  print('Num1 jest niepodzielny przez num2')
